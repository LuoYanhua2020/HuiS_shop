page {
  height: 100%;
}
page .ClassIfParent {
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
page .ClassIfParent .Classif-Left {
  width: 180rpx;
  height: 100%;
  background-color: #FFFFFF;
}
page .ClassIfParent .Classif-Left view {
  height: 85rpx;
  line-height: 85rpx;
  font-size: 35rpx;
  color: #555;
  text-align: center;
}
page .ClassIfParent .Classif-Left .active {
  border-left: 2px solid red;
  background-color: #f5f5f5;
  color: red;
}
page .ClassIfParent .Classif-Right {
  width: 570rpx;
  height: 100%;
  padding-top: 10rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
page .ClassIfParent .Classif-Right .Right-parent {
  width: 100%;
  margin: 20rpx 0;
  box-sizing: border-box;
}
page .ClassIfParent .Classif-Right .Right-parent text {
  display: block;
  font-size: 35rpx;
  margin: 20rpx 0;
}
page .ClassIfParent .Classif-Right .Right-parent .Right-content {
  width: 100%;
  padding: 10rpx;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: start;
          justify-content: start;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background-color: #FFFFFF;
}
page .ClassIfParent .Classif-Right .Right-parent .Right-content view {
  width: 230rpx;
  height: 290rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  margin: 20rpx 20rpx;
  box-sizing: border-box;
}
page .ClassIfParent .Classif-Right .Right-parent .Right-content view image {
  width: 230rpx;
  height: 240rpx;
}
page .ClassIfParent .Classif-Right .Right-parent .Right-content view ._p {
  font-size: 30rpx;
  margin: 15rpx 0;
}

