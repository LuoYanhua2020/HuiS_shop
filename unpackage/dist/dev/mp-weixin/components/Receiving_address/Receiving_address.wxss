page {
  background-color: #FFFFFF;
}
.ReceivingAddress {
  width: 750rpx;
}
.ReceivingAddress .Receiving_null {
  width: 100%;
  height: 700rpx;
  position: relative;
}
.ReceivingAddress .Receiving_null image {
  width: 180rpx;
  height: 180rpx;
  position: absolute;
  left: 50%;
  bottom: 0rpx;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.ReceivingAddress .BottomBth {
  width: 100%;
  height: 90rpx;
  position: fixed;
  left: 0rpx;
  bottom: 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.ReceivingAddress .BottomBth navigator {
  width: 90%;
  height: 100%;
  font-size: 20px;
  color: #fff;
  background-color: #fa436a;
  text-align: center;
  padding-top: 20rpx;
  box-sizing: border-box;
  border-radius: 10px;
}
.ReceivingAddress .Receiving_Number {
  width: 100%;
  border-top: 1px solid rgba(0, 0, 0, 0.15);
}
.ReceivingAddress .Receiving_Number .Receivings {
  width: 100%;
  padding: 15rpx 20rpx;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: 1px solid rgba(0, 0, 0, 0.15);
}
.ReceivingAddress .Receiving_Number .Receivings .ReceivingsLeft {
  width: 13%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.ReceivingAddress .Receiving_Number .Receivings .ReceivingsLeft text {
  color: red;
  font-size: 20rpx;
  border: 1px solid red;
  padding: 7rpx;
  box-sizing: border-box;
}
.ReceivingAddress .Receiving_Number .Receivings .Receivings_left {
  width: 75%;
}
.ReceivingAddress .Receiving_Number .Receivings .Receivings_left .Adress_top {
  width: 100%;
  padding: 5rpx 0;
}
.ReceivingAddress .Receiving_Number .Receivings .Receivings_left .Adress_top text {
  font-size: 30rpx;
  margin-right: 20rpx;
}
.ReceivingAddress .Receiving_Number .Receivings .Receivings_left .Adress_bottom {
  width: 100%;
  padding: 15rpx 0;
}
.ReceivingAddress .Receiving_Number .Receivings .Receivings_left .Adress_bottom text {
  font-size: 28rpx;
  color: #909399;
}
.ReceivingAddress .Receiving_Number .Receivings .Receivings_left .Adress_bottom text:nth-child(1) {
  margin-right: 15rpx;
}
.ReceivingAddress .Receiving_Number .Receivings .Receivings_right {
  width: 18%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: space-evenly;
  -webkit-justify-content: space-evenly;
          justify-content: space-evenly;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}

